#: oracle.c:69
#, c-format
msgid "Guess a number from 1 to 100\n"
msgstr "Загадайте число от 1 до 100"

#: oracle.c:67
#, c-format
msgid "Guess a number from I to C\n"
msgstr "Загадайте число от I до C\n"

#: oracle.c:76
#, c-format
msgid "Is your number greater than %d?\n"
msgstr "Ваше число больше чем %d?\n"

#: oracle.c:74
#, c-format
msgid "Is your number greater than %s?\n"
msgstr "Ваше число больше чем %s?"

#: oracle.c:7
msgid ""
"Program for guessing number in arabic or roman notation\n"
"\n"
"Usage: oracle [OPTIONS]\n"
"\n"
"\t--help\t\tprint this message\n"
"\t-r\t\troman notation mode\n"
msgstr ""
"Программа отгадывает число в арабской или римской записи\n"
"\n"
"Использование: oracle [OPTIONS]\n"
"\n"
"\t--help\t\tвывести это сообщение\n"
"\t-r\t\tримская запись чисел\n"

#: oracle.c:84
#, c-format
msgid "Understand you not. Try again!\n"
msgstr "Не понимать. Повторите!"

#: oracle.c:87
#, c-format
msgid "Your number is %d\n"
msgstr "Ваше число %d\n"

#: oracle.c:77 oracle.c:81
msgid "no"
msgstr "нет"

#: oracle.c:77 oracle.c:79
msgid "yes"
msgstr "да"
